      <xsd:element minOccurs="1" name="aduana">
        <xsd:annotation>
          <xsd:documentation>Clave de la Aduana/Sección en la que se realizó
						la operación de Comercio Exterior.</xsd:documentation>
        </xsd:annotation>
        <xsd:simpleType>
          <xsd:restriction base="xsd:int">
            <xsd:minInclusive value="1" />
            <xsd:maxInclusive value="999" />
