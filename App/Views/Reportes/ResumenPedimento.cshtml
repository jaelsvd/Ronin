@model IEnumerable<App.Entities.Reportes.ResumenPedimento>

@{
    ViewBag.Title = "Pedimentos";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>Reporte Resumen Pedimento</h2>
        <ol class="breadcrumb">
            <li>
                <a href="">Reportes</a>
            </li>
            <li class="active">
                <strong>Resumen Pedimento</strong>
            </li>
        </ol>
    </div>
    <div class="col-lg-2">

    </div>
</div>
<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">

                <div class="ibox-content">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover dataTables-example">
                            <thead>
                                <tr>
                                    <th>
                                        Número Pedimento
                                    </th>                                  
                                    <th>
                                       Operación
                                    </th>
                                    <th>
                                       Clave
                                    </th>
                                    <th>
                                        Patente
                                    </th>
                                    <th>
                                        Fecha Pago
                                    </th>
                                    <th>
                                        V. Aduana
                                    </th>
                                    <th>
                                       Cantidad Partidas
                                    </th>
                                    <th>
                                        IVA
                                    </th>
                                    <th>
                                        DTA Pagado
                                    </th>
                                    <th>
                                        Total Multas
                                    </th>
                                    <th>
                                        PRV
                                    </th>
                                    <th>
                                        IGI
                                    </th>
                                    <th>
                                        Contribución Efectivo
                                    </th>
                                    <th>
                                        CURP
                                    </th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>
                                            <a href="@Url.Action("DetallePedimento", "Pedimento", new { id = @Html.DisplayFor(modelItem => item.Id) })"> @Html.DisplayFor(modelItem => item.NumPedimento)</a>
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Operacion)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Clave)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Patente)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.FechaPago)    
                                        </td>                                                                                                  
                                        <td>
                                            @Html.DisplayFor(modelItem => item.ValorAduana)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.CantidadPartidas)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.Iva)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.DTA)
                                        </td>
                                        <td>
                                           @Html.DisplayFor(modelItem => item.Multas)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.PRV)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.IGIIGE)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.ContribucionEfectivo)
                                        </td>
                                        <td>
                                            @Html.DisplayFor(modelItem => item.CURP)
                                        </td>
                                    </tr>
                                }
                            </tbody>

                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
