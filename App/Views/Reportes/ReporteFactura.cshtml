@model IEnumerable<App.Entities.Pedimento>

@{
    ViewBag.Title = "Facturas COVE";
}

<div class="row wrapper border-bottom white-bg page-heading">
    <div class="col-lg-10">
        <h2>Reporte Facturas COVE</h2>
        <ol class="breadcrumb">
            <li>
                <a href="">Reportes</a>
            </li>
            <li class="active">
                <strong>Facturas COVE</strong>
            </li>
        </ol>
    </div>
    <div class="col-lg-2">

    </div>
</div>

<div class="wrapper wrapper-content animated fadeInRight">
    <div class="row">
        <div class="col-lg-12">
            <div class="ibox float-e-margins">

                <div class="ibox-content">
                    <div class="table-responsive">
                        <table class="table table-striped table-bordered table-hover dataTables-example">
                            <thead>
                                <tr>
                                    <th>Patene</th>
                                    <th>Pedimento</th>
                                    <th>Aduana</th>
                                    <th>Clave</th>
                                    <th>Tipo Operacion</th>
                                    <th>Fecha Pago</th>
                                    <th>Rectificado Por Pedimento</th>
                                    <th>Rectifica Pedimento</th>
                                    <th>Factura</th>
                                    <th>COVE</th>
                                    <th>ID Emisor</th>
                                    <th>Emisor</th>
                                    <th>ID Destinatario</th>
                                    <th>Destinatario</th>
                                    <th>Fecha Pago</th>
                                </tr>
                            </thead>
                            <tbody>
                                @{ foreach (var item in Model)
                                    {
                                        <tr>
                                            <td>@Html.DisplayFor(modelItem => item.Patente)</td>
                                            <td>@Html.DisplayFor(modelItem => item.NumPedimento)</td>
                                            <td>@Html.DisplayFor(modelItem => item.Aduana)</td>
                                            <td>@Html.DisplayFor(modelItem => item.EncabezadoClavDocClave)</td>
                                            <td>@Html.DisplayFor(modelItem => item.EncabezadoOpDesc)</td>
                                            <td>
                                               
                                            </td>
                                            <td>

                                            </td>
                                            <td>

                                            </td>
                                            <td>
                                                
                                            </td>
                                            <td>
                                                @foreach (var factura in item.PedimentoFactura)
                                                {
                                                    @Html.DisplayFor(modelItem => factura.NumeroFactura)
                                                    <br />
                                                }
                                            </td>
                                            <td></td>
                                            <td> @foreach (var factura in item.PedimentoFactura)
                                            {
                                                @Html.DisplayFor(modelItem => factura.ProveedorComp)
                                                <br />
                                            }</td>
                                            <td></td>
                                            <td></td>
                                            <td>
                                                @foreach (var factura in item.PedimentoFactura)
                                                {
                                                    @Html.DisplayFor(modelItem => factura.FechaFacturacion)
                                                    <br />
                                                }
                                            </td>
                                        </tr>
                                    }
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
